<!DOCTYPE html>
<html>

<head>
    <meta name="viewport" charset="utf-8" content="initial-scale=1, maximum-scale=1, column-scalable=no" />
    <link rel="stylesheet" href="/stylesheets/angular-material.css">
    <script src="/javascripts/angular.js"></script>
    <script src="/javascripts/angular-aria.js"></script>
    <script src="/javascripts/angular-animate.js"></script>
    <script src="/javascripts/angular-material.js"></script>
    <!-- <script src="./server.js"></script> -->  


</head>

<body ng-app="index">
    <div ng-controller="indexController" layout="column" ng-cloak class="md-inline-form">  
      <!-- The top toolbar -->
    <md-toolbar>
      <div class="md-toolbar-tools">
      
       <md-button class="md-icon-button" aria-label="Radiation">
          <md-icon md-svg-icon="/images/Radiation.svg" ></md-icon>
        </md-button>
        <h2>
          <span>Welcome</span>
        </h2>
        <!-- <md-button ng-click="loadfile()">load file</md-button> -->
        <!-- <md-button aria-label="Go Back" ng-click="loadfile()">load file</md-buttom> -->
        

      </div>
    </md-toolbar>
      <md-content class="md-padding" layout-xs="column" layout="row">
    <div flex-xs flex-gt-xs="50" layout="column">
      <md-card>
        <md-card-title>
          <md-card-title-text>
            <span class="md-headline">Create from import</span>
            <span class="md-subhead">Import config file</span>
          </md-card-title-text>
          <md-card-title-media>
            <div class="md-media-lg card-media"></div>
          </md-card-title-media>
        </md-card-title>
        <md-card-actions layout="row" layout-align="end center">
          <md-button>Action 1</md-button>
          <md-button>Action 2</md-button>
        </md-card-actions>
      </md-card>
      
    </div>
    <div flex-xs flex-gt-xs="50" layout="column">
     <md-card>
        <md-card-title>
          <md-card-title-text>
            <span class="md-headline">Manually Create</span>
            <span class="md-subhead">Create Meta-Settings files from Beginning.</span>
          </md-card-title-text>
          <md-card-title-media>
            <div class="md-media-lg card-media"></div>
          </md-card-title-media>
        </md-card-title>
        <md-card-actions layout="row" layout-align="end center">
          <md-button ng-click ="goManually()">GO!</md-button>
          
        </md-card-actions>
      </md-card>
    </div>
  </md-content>

    </div>   
</body>
<script src="/javascripts/indexcontroller.js"></script>

</html>